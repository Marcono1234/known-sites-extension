{
  "manifest_version": 2,
  "name": "Known Sites",
  "description": "__MSG_manifest_extension_description__",
  "homepage_url": "https://github.com/Marcono1234/known-sites-extension",
  "default_locale": "en",
  "version": "1.0.1",
  "browser_specific_settings": {
    "gecko": {
      "id": "known-sites@marcono1234.invalid"
    }
  },
  "icons": {
    "48": "icon.svg",
    "96": "icon.svg"
  },
  "browser_action": {
    "browser_style": true,
    "default_area": "navbar",
    "default_icon": "icon.svg",
    "default_title": "__MSG_browser_action_title__"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "history",
    "bookmarks",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "web-ext-polyfill/browser-polyfill.js",
      "background-scripts/background.js"
    ],
    "persistent": true
  },
  "web_accessible_resources": ["pages/blocked-unknown.html", "icon.svg"]
}
